
<head>
  <meta charset="UTF-8">
  <title>Shopping List</title>
  <%- include('header'); %>
  <li><a href="/cart">カートへ</a></li>
</head>
<body>
  <main>
    <ul class="omusubilist">
      <% omusubilist.forEach((omusubi) => { %>
        <li>
          <% if (omusubi[2] === '期間限定') {%>
            <i>期間限定</i> 
          <% }else{ %> 
            <i>定番</i> 
            <%} %>           
          <name><%= omusubi[0] %></name>
          <price><%= omusubi[1] %>円</price>
          <% if (omusubi[3] === 0) {%>
            <i>売り切れ</i> 
          <% }else{ %> 
            <i>残数<%= omusubi[3] %></i> 
          <%} %>   
          <form action="/list" method="post">
            <button type="submit" name="buyingOmusubi" value= <%- omusubi[0] %>>カートに追加</button>
          </form>
        </li>
        <% }) %>
    </ul>
  </main>
  
</body>
</html>
